        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "crowd_certain/utilities/__pycache__": true,
        "requirements.yml": true,
        "requirements.txt": true,
        "needs_profiling.prof": true,
